    "@nuxt/eslint-config": "^0.7.3",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.13.1",
    "@types/accept-language-parser": "^1.5.6",
