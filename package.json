    "@tiptap/extension-underline": "2.1.13",
    "@tiptap/pm": "2.1.13",
    "@tiptap/starter-kit": "2.1.13",
    "@vueuse/integrations": "^10.11.0",
    "accept-language-parser": "^1.5.0",
    "argon2": "^0.31.2",
    "c12": "^1.11.1",
    "citty": "^0.1.6",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "dayjs": "^1.11.11",
    "defu": "^6.1.4",
    "dompurify": "^3.1.5",
    "dot-prop": "^8.0.2",
    "execa": "^7.2.0",
    "flatpickr": "^4.6.13",
    "focus-trap": "^7.5.4",
    "fs-extra": "^11.2.0",
    "giget": "^1.2.3",
    "jiti": "^1.21.6",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "listhen": "^1.9.0",
    "magicast": "^0.3.5",
    "mime": "^3.0.0",
    "nanoid": "^4.0.2",
    "node-ssh": "^13.2.0",
    "nypm": "^0.3.9",
