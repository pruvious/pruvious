    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.4.4",
    "typescript": "latest",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.1.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": ">=20"
  }
}
