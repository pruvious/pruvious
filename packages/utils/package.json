{
  "name": "@pruvious/utils",
  "description": "Compilation of JavaScript utility functions and TypeScript types.",
  "version": "2.0.0",
  "homepage": "https://pruvious.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pruvious/pruvious.git",
    "directory": "packages/utils"
  },
  "author": "Muris Ceman <hello@pruvious.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./path-matcher": {
      "import": "./dist/misc/PathMatcher.mjs",
      "types": "./dist/misc/PathMatcher.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "unbuild && node scripts/readme.mjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "defu": "6.1.4",
    "dot-prop": "9.0.0",
    "nanoid": "5.1.5",
    "ohash": "1.1.6",
    "zeptomatch": "2.0.1"
  },
  "devDependencies": {
    "typescript": "5.8.2",
    "unbuild": "3.5.0"
  }
}
